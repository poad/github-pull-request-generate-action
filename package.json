{
  "name": "github-pull-request-generate-action",
  "private": true,
  "version": "1.1.2",
  "repository": "https://github.com/poad/github-pull-request-generate-action.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "main": "./dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "format-check": "rome format src",
    "format": "rome format --write src",
    "check": "rome check src",
    "check-fix": "rome check --apply src",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "clean": "rm -rf dist",
    "prepackage": "pnpm build",
    "prebuild": "pnpm clean && pnpm lint-fix",
    "preclean": "pnpm install"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@swc/core": "^1.3.73",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.6.2",
    "jest-circus": "^29.6.2",
    "nock": "^13.3.2",
    "regenerator-runtime": "^0.13.11",
    "rome": "^12.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/core": "^4.2.4",
    "@octokit/graphql": "^7.0.1",
    "graphql": "^16.7.1",
    "node-fetch": "^3.3.2"
  },
  "peerDependencies": {
    "@actions/core": "*",
    "@octokit/core": "*",
    "graphql": "*"
  },
  "exports": {
    ".": {
      "node": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?.+)"
    ]
  }
}