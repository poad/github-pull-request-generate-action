{
  "name": "github-pull-request-generate-action",
  "private": true,
  "version": "1.1.2",
  "repository": "https://github.com/poad/github-pull-request-generate-action.git",
  "author": "Kenji Saito <ken-yo@mbr.nifty.com>",
  "license": "MIT",
  "main": "./dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "format-check": "biome format src",
    "format": "biome format --write src",
    "check": "biome check src",
    "check-fix": "biome check --apply-unsafe src",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "clean": "rm -rf dist",
    "prepackage": "pnpm build",
    "prebuild": "pnpm clean && pnpm lint-fix",
    "preclean": "pnpm install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.0.0",
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "nock": "^13.3.4",
    "regenerator-runtime": "^0.13.11",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@octokit/core": "^4.2.4",
    "@octokit/graphql": "^7.0.2",
    "graphql": "^16.8.1"
  },
  "peerDependencies": {
    "@actions/core": "*",
    "@octokit/core": "*",
    "graphql": "*"
  },
  "exports": {
    ".": {
      "node": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?.+)"
    ]
  },
  "packageManager": "pnpm@8.7.1",
  "engines": {
    "npm": "use pnpm please!",
    "pnpm": ">=8.7.1"
  }
}